<h1>Session#new</h1>
<p>Find me in app/views/session/new.html.erb</p>

<%= form_tag login_path do %>
    <p>
        <%= label_tag(:email) %>
        <%= email_field_tag(:email) %>
    </p>
    <p>
        <%= label_tag(:password) %>
        <%= password_field_tag(:password) %>
    </p>

    <%= submit_tag('Create account') %>
<% end %>
